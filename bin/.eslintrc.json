{
	"root": true,
	"parserOptions": {
		"ecmaVersion": 2020
	},
	"env": {
		"node": true
	},
	"rules": {
		"semi": "error",
		"no-extra-semi": "warn",
		"curly": "warn",
		"quotes": ["error", "single", { "allowTemplateLiterals": true }],
		"eqeqeq": "error",
		"indent": ["warn", "tab", { "SwitchCase": 1 }]
	}
}
